{
    "name": "eslint-config-productsway",
    "version": "1.1.1",
    "keywords": [
        "javascript",
        "typescript",
        "eslint",
        "config",
        "xojs",
        "prettier"
    ],
    "license": "MIT",
    "author": "Huynh Duc Dung",
    "exports": {
        ".": "./src/typescript.js",
        "./typescript": "./src/typescript.js",
        "./react": "./src/react.js"
    },
    "main": "src/typescript.js",
    "files": [
